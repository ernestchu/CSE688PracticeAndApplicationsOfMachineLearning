   "source": [
    "env = snake_env()\n",
    "while True:\n",
    "    env.reset()\n",
    "    while True:\n",
    "        env.render()\n",
    "        delay(.3)\n",
    "        next_state, reward, done, info = env.step(random.randint(0, 3))\n",
    "        print(next_state)\n",
    "        print(f'Reward: {reward}')\n",
    "        print(done)\n",
    "        print(info)\n",
    "        if done:\n",
    "            break"
   ]
